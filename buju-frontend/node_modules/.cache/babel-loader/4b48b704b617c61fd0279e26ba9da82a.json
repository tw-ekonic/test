his[name].length) {\n        this[array].push(this[name]);\n        this[name] = '';\n      }\n    },\n    onResultSubmit(result) {\n      this.results.push(result);\n      console.log(this.results);\n    },\n    deleteStation(station) {\n      this.stations = this.stations.filter((sta) => sta !== station);\n    },\n    deleteStudent(student) {\n      this.students = this.students.filter((stu) => stu !== student);\n    },\n  },\n  components: {\n    StationsTable,\n    StudentsTable,\n    ResultsTable,\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: \"Montserrat\";\n}\n</style>\n"],"sourceRoot":""}]}