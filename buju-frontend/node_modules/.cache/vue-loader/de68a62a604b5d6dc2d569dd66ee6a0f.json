\n  name: 'App',\n  data() {\n    return {\n      stations: [],\n      stationName: '',\n      students: [],\n      studentName: '',\n      results: [],\n      resultName: '',\n    }\n  },\n  methods: {\n    onInputChange(e, value) {\n      this[value] = e.target.value;\n    },\n    onSubmit(array, name) {\n      if (this[name].length) {\n        this[array].push(this[name]);\n        this[name] = '';\n      }\n    },\n    onResultSubmit(result) {\n      this.results.push(result);\n      console.log(this.results);\n    },\n    deleteStation(station) {\n      this.stations = this.stations.filter((sta) => sta !== station);\n    },\n    deleteStudent(student) {\n      this.students = this.students.filter((stu) => stu !== student);\n    },\n  },\n  components: {\n    StationsTable,\n    StudentsTable,\n    ResultsTable,\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: \"Montserrat\";\n}\n</style>\n"]}]}