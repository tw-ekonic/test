                 v-bind:delete-station=\"deleteStation\"\n                   v-bind:stations=\"stations\"\n                   v-bind:on-submit=\"onSubmit\"\n                   v-bind:stationName=\"stationName\"\n    />\n  </div>\n  <ResultsTable />\n</template>\n\n<script>\nimport StationsTable from \"@/components/StationsTable\";\nimport StudentsTable from \"@/components/StudentsTable\";\nimport ResultsTable from \"@/components/ResultsTable\";\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      stations: [],\n      stationName: '',\n      students: [],\n      studentName: '',\n    }\n  },\n  methods: {\n    onInputChange(e, value) {\n      this[value] = e.target.value;\n    },\n    onSubmit(array, name) {\n      if (this[name].length) {\n        this[array].push(this[name]);\n        this[name] = '';\n      }\n    },\n    deleteStation(station) {\n      this.stations = this.stations.filter((sta) => sta !== station);\n    },\n    deleteStudent(student) {\n      this.students = this.students.filter((stu) => stu !== student);\n    },\n  },\n  components: {\n    StationsTable,\n    StudentsTable,\n    ResultsTable,\n  }\n}\n</script>\n\n<style>\n  #app {\n    font-family: \"Montserrat\";\n  }\n</style>\n"]}]}